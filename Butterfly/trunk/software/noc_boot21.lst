                                        ;----------------------------------------------------------------------------
                                        ; Receive from network.
                                        ; Receive status must have already indicated a message present.
                                        ; Copies recieve buffer from router to rxBuf.
                                        ; The router fifo is configured with first word fall-through. This means
                                        ; the data word appears on the fifo output before the fifo is popped.
                                        ; A fifo pop signal has to be sent after reading the data.
                                        ;
                                        ; Parameters:
                                        ;	<none>
                                        ; Returns:
                                        ;	<none>
                                        ; Registers Affected:
                                        ;	<none>
                                        ;----------------------------------------------------------------------------
                                        
    274 0000E06A                        Recv:
    275 0000E06A FC 6E                   		add		sp,sp,#-4
    276 0000E06C E0 D1                   		sw		r1,[sp]
    277 0000E06E E2 D2                   		sw		r2,2[sp]
                                        		; Pop the rx fifo.
                                        		; This bit $40 should automatically clear so we don't need to
                                        		; set it back to zero.
    281 0000E070 01 4B 01 E1             		lb		r1,ROUTER+RTR_RXCTL
    282 0000E074 04 40 60 31             		or		r1,#$40
    283 0000E078 01 4B 01 C1             		sb		r1,ROUTER+RTR_RXCTL
    284 0000E07C 0B 40 5F 31             		and		r1,#$BF
    285 0000E080 01 4B 01 C1             		sb		r1,ROUTER+RTR_RXCTL
    286 0000E084 00 40 0F 12             		lw		r2,#15
    287 0000E088                        Recv1:
    288 0000E088 00 4B 20 E1             		lb		r1,ROUTER[r2]			; copy message to local buffer
    289 0000E08C 03 40 20 C1             		sb		r1,rxBuf[r2]
    290 0000E090 FF 62                   		add		r2,r2,#-1
    291 0000E092 FA BB                   		bpl		Recv1
    292 0000E094 E0 F1                   		lw		r1,[sp]
    293 0000E096 E2 F2                   		lw		r2,2[sp]
    294 0000E098 04 6E                   		add		sp,sp,#4
    295 0000E09A F0 80                   		ret
                                        
