                        	;------------------------------------------------------------------------------
                        	; Insert task into ready list. The list is a doubly linked circular list.
                        	;
                        	; Parameters:
                        	;		a0 = tid to insert
                        	; Modifies:
                        	;		v1,t0,t1,t2
                        	; Returns:
                        	;		v0 = 1 for success, 0 if failed
                        	;------------------------------------------------------------------------------
                        	
                        	InsertIntoReadyList:
FFFC1F74 00006813		ldi		$v0,#E_Ok						; setup for ok
FFFC1F78 00F06893		ldi		$v1,#MAX_TID				; check argument
FFFC1F7C 0928E663		bgtu	$a0,$v1,.badTid
FFFC1F80 00A91E93		sll		$t3,$a0,#LOG_TCBSZ	; tid to pointer
FFFC1F84 288ECD03		ldbu	$t0,TCBStatus[$t3]	; set status to ready
FFFC1F88 001D6D13		or		$t0,$t0,#TS_READY
FFFC1F8C 29AE8423		sb		$t0,TCBStatus[$t3]
FFFC1F90 289E8D03		ldb		$t0,TCBPriority[$t3]
FFFC1F94 003D7D13		and		$t0,$t0,#3					; limit to four
FFFC1F98 001D1D13		sll		$t0,$t0,#1					; *2 for indexing
FFFC1F9C 00004637		ldw		$t1,READYQ[$t0]			; get head of queue for that priority
FFFC1FA0 30860613
FFFC1FA4 01A60633
FFFC1FA8 00061D83
FFFC1FAC 020DD063		bge		$t1,$x0,.insert			; Is there a head?
                        		; level 0 insert - nothing in list, create a ring to self
FFFC1FB0 00004637		stw		$a0,READYQ[$t0]			; no head, simple to insert
FFFC1FB4 30860613
FFFC1FB8 01A60633
FFFC1FBC 01261023
FFFC1FC0 2D2E9823		stw		$a0,TCBNext[$t3]		; remember it's a ring
FFFC1FC4 2D2E9C23		stw		$a0,TCBPrev[$t3]
FFFC1FC8 00008067		ret
                        		; Insert at tail of list, which is just before the head.
                        	.insert:
FFFC1FCC 00AD9F13		sll		$t4,$t1,#LOG_TCBSZ	; tid to pointer
FFFC1FD0 2D8F1E03		ldw		$t2,TCBPrev[$t4]		; t2 = head->prev
FFFC1FD4 01BE1C63		bne		$t2,$t1,.level2
                        		;level 1 insert - only 1 item currently in list, items point to each other
                        		; This bit probably redundant code as normal insertion should work as well.
FFFC1FD8 2D2F1C23		stw		$a0,TCBPrev[$t4]		; simple to insert
FFFC1FDC 2D2F1823		stw		$a0,TCBNext[$t4]		; head->prev = head->next = arg
FFFC1FE0 2DBE9C23		stw		$t1,TCBPrev[$t3]		; arg->prev = arg->next = head
FFFC1FE4 2DBE9823		stw		$t1,TCBNext[$t3]
FFFC1FE8 00008067		ret
                        		; Here there will be three or more entries in the list. Arg is inserted
                        		; between head and head->prev to make it the last on the list.
                        	.level2:
FFFC1FEC 2DCE9C23		stw		$t2,TCBPrev[$t3]		; arg->prev = head->prev
FFFC1FF0 2DBE9823		stw		$t1,TCBNext[$t3]		; arg->next = head, arg links are now set
                        		; If there's a head there should be a head->prev and head->next at least pointing to the head
FFFC1FF4 000E4663		bltz	$t2,.0001						; head->prev valid?
FFFC1FF8 00AE1E93		sll		$t3,$t2,#LOG_TCBSZ	; t3 = head->prev (as a pointer)
FFFC1FFC 2D2E9823		stw		$a0,TCBNext[$t3]		; head->prev->next = arg
                        	.0001:
FFFC2000 2D2F1C23		stw		$a0,TCBPrev[$t4]		; head->prev = arg
FFFC2004 00008067		ret
                        	.badTid:
FFFC2008 00106813		ldi		$v0,#E_Arg
FFFC200C 00008067		ret
                        	
                        	;------------------------------------------------------------------------------
                        	; The ready list is a doubly linked list to make it easier and faster to
                        	; remove items from the list. There's no need to search the list for the item
                        	; to remove.
                        	;
                        	; Parameters:
                        	;		a0 = tid to remove
                        	; Modifies:
                        	;		t0,t1,t2,t3,t4,t5
                        	; Returns:
                        	;		v0 = E_Ok for success
                        	;------------------------------------------------------------------------------
                        	
                        	RemoveFromReadyList:
FFFC2010 00F06893		ldi		$v1,#MAX_TID					; check arg
FFFC2014 0928E063		bgtu	$a0,$v1,.badTid
FFFC2018 00A91F13		sll		$t4,$a0,#LOG_TCBSZ		; s1 = pointer to tcb
FFFC201C 288F0D03		ldb		$t0,TCBStatus[$t4]		; set status no longer ready or running
FFFC2020 F7ED7D13		and		$t0,$t0,#~(TS_READY|TS_RUNNING)
	stb		$t0,TCBStatus[$t4]
FFFC2024 29AF0423
FFFC2028 289F0D03		ldb		$t0,TCBPriority[$t4]	; t0 = priority
FFFC202C 003D7D13		and		$t0,$t0,#3						; limit to 0-3
FFFC2030 001D1D13		sll		$t0,$t0,#1						; *2 for indexing
FFFC2034 00004637		ldw		$t1,READYQ[$t0]				; get head tid
FFFC2038 30860613
FFFC203C 01A60633
FFFC2040 00061D83
FFFC2044 2D0F1E03		ldw		$t2,TCBNext[$t4]			; get arg->next
FFFC2048 012D9E63		bne		$t1,$a0,.0001					; removing head of list?
FFFC204C 012E1463		bne		$t2,$a0,.0003					; list emptying? (if arg->next points back to arg)
FFFC2050 FFF06E13		ldi		$t2,#-1
                        	.0003:
FFFC2054 00004637		stw		$t2,READYQ[$t0]				; yes, set new head to arg->next
FFFC2058 30860613
FFFC205C 01A60633
FFFC2060 01C61023
                        	.0001:
FFFC2064 000E4E63		bltz	$t2,.0002							; validate t2 (arg->next)
FFFC2068 011E7C63		bgeu	$t2,$v1,.0002					; there should always be an arg->next, arg->prev
FFFC206C 2D8F1E83		ldw		$t3,TCBPrev[$t4]			; because the list is circular t3=arg->prev
FFFC2070 00AE9F93		sll		$t5,$t3,#LOG_TCBSZ		; t5 = arg->prev as a pointer
FFFC2074 2DCF9823		stw		$t2,TCBNext[$t5]			; arg->prev->next = arg->next
FFFC2078 00AE1F93		sll		$t5,$t2,#LOG_TCBSZ		; t5 = arg->next as a pointer
FFFC207C 2DDF9C23		stw		$t3,TCBPrev[$t5]			; arg->next->prev = arg->prev
                        		; Now indicate links in TCB are not in use.
                        	.0002:
FFFC2080 FFF06813		ldi		$v0,#-1
FFFC2084 2D0F1823		stw		$v0,TCBNext[$t4]
FFFC2088 2D0F1C23		stw		$v0,TCBPrev[$t4]
FFFC208C 00006813		ldi		$v0,#E_Ok							; we're ok
FFFC2090 00008067		ret
                        	.badTid:
FFFC2094 00106813		ldi		$v0,#E_Arg
FFFC2098 00008067		ret
                        	
                        	;------------------------------------------------------------------------------
                        	; Parameters:
                        	;		a0 = task id to insert
                        	;		a1 = timeout value
                        	; Modifies:
                        	;		t0,t1,t2,t3,t4,t5
                        	;------------------------------------------------------------------------------
                        	
                        	InsertIntoTimeoutList:
FFFC209C 1139000D		setto	$a0,$a1
	ret
FFFC20A0 00008067
FFFC20A4 00A91F93		sll		$t5,$a0,#LOG_TCBSZ		; tid to pointer
FFFC20A8 FFF06D13		ldi		$t0,#-1						
FFFC20AC 2DAF9823		stw		$t0,TCBNext[$t5]			; initialize indexes to -1
FFFC20B0 2DAF9C23		stw		$t0,TCBPrev[$t5]
FFFC20B4 00004637		ldw		$t0,TimeoutList
FFFC20B8 32860613
FFFC20BC 00060633
FFFC20C0 00061D03
FFFC20C4 020D5063		bge		$t0,$x0,.0001
                        		; Here the timeout list was empty, easy to insert.
FFFC20C8 2F3FA023		stt		$a1,TCBTimeout[$t5]		; set timeout for task
FFFC20CC 00004637		stw		$a0,TimeoutList				; set head of list
FFFC20D0 32860613
FFFC20D4 00060633
FFFC20D8 01261023
FFFC20DC 00006813		ldi		$v0,#E_Ok							; and return
FFFC20E0 00008067		ret
                        	.0001:
FFFC20E4 FFF06D93		ldi		$t1,#-1								; t1 used as previous index
FFFC20E8 000D6E33		mov		$t2,$t0								; t2 = head of timeout list index
FFFC20EC 00AE1E93		sll		$t3,$t2,#LOG_TCBSZ		; t3 = head of timeout list as a pointer
                        		; Search the timeout list for the proper insertion point. The timeouts are
                        		; relative, so the requested amount is decremented by the amounts in the
                        		; list as it is searched.
                        	.beginWhile:
FFFC20F0 2E0EAF03		ldt		$t4,TCBTimeout[$t3]		; get the timeout
FFFC20F4 033F5663		ble		$a1,$t4,.endWhile			; is it greater than the requested timeout ?
FFFC20F8 41E989B3		sub		$a1,$a1,$t4						; decrement by list amount
FFFC20FC 000EEDB3		mov		$t1,$t3								; t1 = pointer to previous entry
FFFC2100 2D0E9E83		ldw		$t3,TCBNext[$t3]			; get the next entry in list
FFFC2104 000EC663		bltz	$t3,.endOfList					; end of list?
FFFC2108 00AE9E93		sll		$t3,$t3,#LOG_TCBSZ		; no, set pointer to current entry
FFFC210C FFBE92E3		bne		$t3,$t1,.beginWhile		; list screwed up? (This test should always be true)
                        	.endOfList:
                        		; Here the end of the list was reached, insert as last entry
FFFC2110 2D2D9823		stw		$a0,TCBNext[$t1]			; entry->next = arg
FFFC2114 2DBF9C23		stw		$t1,TCBPrev[$t5]			; arg->prev = entry
FFFC2118 2F3FA023		stt		$a1,TCBTimeout[$t5]
FFFC211C 04000063		bra		.0003
                        	.endWhile:
                        		; Here we found a timeout greater than the requested amount. Insert before.
FFFC2120 20AEDE13		sra		$t2,$t3,#LOG_TCBSZ		; pointer to tid
FFFC2124 2DCF9823		stw		$t2,TCBNext[$t5]			; arg->next = entry
FFFC2128 20ADDE13		sra		$t2,$t1,#LOG_TCBSZ
FFFC212C 2DCF9C23		stw		$t2,TCBPrev[$t5]			; arg->prev = prev entry
FFFC2130 2E0EAE03		ldt		$t2,TCBTimeout[$t3]		; update timeout of entry by subtracting off
FFFC2134 413E0E33		sub		$t2,$t2,$a1						; the arg's timeout
FFFC2138 2FCEA023		stt		$t2,TCBTimeout[$t3]
FFFC213C 2D2E9C23		stw		$a0,TCBPrev[$t3]			; entry->prev = arg
FFFC2140 000DC663		bltz	$t1,.0002							; previous entry? no - go update list head
FFFC2144 2D2D9823		stw		$a0,TCBNext[$t1]			; prev entry->next = arg
FFFC2148 00000A63		bra		.0003
                        	.0002:
FFFC214C 00004637		stw		$a0,TimeoutList				; list head = arg
FFFC2150 32860613
FFFC2154 00060633
FFFC2158 01261023
                        	.0003:
FFFC215C 288FCE03		ldbu	$t2,TCBStatus[$t5]		; set status to timing out
FFFC2160 010E6E13		or		$t2,$t2,#TS_TIMEOUT
FFFC2164 29CF8423		stb		$t2,TCBStatus[$t5]
FFFC2168 00006813		ldi		$v0,#E_Ok
FFFC216C 00008067		ret
                        	
