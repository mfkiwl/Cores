FFFC21C4 FFFE4E13		xor		$t2,$t2,#-1
FFFC21C8 01CDFDB3		and		$t1,$t1,$t2						; clear bit
FFFC21CC 01BD1223		sh		$t1,MBX_WTIDS[$t0]		; update waiting task list
