FFFC2170 1009000D		setto	$a0,$x0
	ret
FFFC2174 00008067
FFFC2178 00A91E93		sll		$t3,$a0,#LOG_TCBSZ		; tid to pointer
FFFC217C 288ECD03		ldbu	$t0,TCBStatus[$t3]		; check if waiting at a mailbox
FFFC2180 008D7D13		and		$t0,$t0,#TS_WAITMSG
FFFC2184 060D0063		beqz	$t0,.noWait				
                        		
FFFC2188 FF070713		sub		$sp,$sp,#16
FFFC218C 00472023		sw		$s1,[$sp]
