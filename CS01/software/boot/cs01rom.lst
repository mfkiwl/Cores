                        	; macros have to be defined before they are encountered.
                        	
                        	; unlock the a semaphore	
                        	macro mUnlockSemaphore (adr)
	ldi		v0,#-1
	sw		v0,adr
endm
                        	
                        	; Look at the asid register for task id
                        	macro mGetCurrentTid
	csrrw	v0,#$181,x0
	and		v0,v0,#15
endm
                        	
                        	macro	mHasFocus
	ldi		a0,#20
	ecall
endm
                        	
                        	macro mSleep(tm)
	ldi		a0,#5
	ldi		a1,#tm
	ecall
endm
                        	
