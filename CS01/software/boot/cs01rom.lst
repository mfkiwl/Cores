                        	;-----------------------------------------------------------------------------
                        	; Determine if the currently running task has the I/O focus.
                        	;
                        	; Stack Space:
                        	;		2 words
                        	; Parameters:
                        	;		none
                        	; Modifies:
                        	;		none
                        	; Returns:
                        	;		v0 = E_Ok
                        	;		v1 = 1 if task has IO focus, 0 otherwise
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_HasIOFocus:
                        		
FFFC2500 18101873		csrrw	v0,#$181,x0
FFFC2504 00F87813		and		v0,v0,#15
FFFC2508 00A81813	sll		$v0,$v0,#LOG_TCBSZ
FFFC250C 00009637		ldt		$v1,IOFocusNdx
FFFC2510 C0860613
FFFC2514 00060633
FFFC2518 00062883
FFFC251C 01180663		beq		$v0,$v1,.hasFocus
FFFC2520 00006893		ldi		$v1,#0
FFFC2524 00000463		bra		.xit
                        	.hasFocus:
FFFC2528 00106893		ldi		$v1,#1
                        	.xit:
FFFC252C 00006813		ldi		$v0,#E_Ok
FFFC2530 EF0FF06F		jmp		ERETx
                        	
                        	;-----------------------------------------------------------------------------
                        	; First check if it's even possible to switch the focus to another
                        	; task. The I/O focus list could be empty or there may be only a
                        	; single task in the list. In either case it's not possible to
                        	; switch.
                        	;
                        	;	Parameters:
                        	;		none
                        	;	Modifies:
                        	;		none
                        	;	Returns:
                        	;		v0 = E_Ok
                        	;-----------------------------------------------------------------------------
                        	
                        	SwitchIOFocusHelper:
FFFC2534 00009637		ldt		$t0,IOFocusNdx			; get focus pointer
FFFC2538 C0860613
FFFC253C 00060633
FFFC2540 00062D03
FFFC2544 020D4063		bltz	$t0,.noFocus		; is it -1?
FFFC2548 2E8D2D83		ldt		$t1,IOF_NEXT[$t0]
FFFC254C 01AD8C63		beq		$t1,$t0,.sameFocus
FFFC2550 000DCA63		bltz	$t1,.noFocus
                        		; swap virtual screens
                        		; set vidmem pointer
FFFC2554 00009637		stt		$t1,IOFocusNdx
FFFC2558 C0860613
FFFC255C 00060633
FFFC2560 01B62023
                        	.sameFocus:
                        	.noFocus:
FFFC2564 00006813		ldi		$v0,E_Ok
FFFC2568 00008067		ret
                        	
                        	FMTK_SwitchIOFocus:
FFFC256C FC9FF0EF		call	SwitchIOFocusHelper
FFFC2570 EB0FF06F		jmp		ERETx
                        	
                        	;-----------------------------------------------------------------------------
                        	; The I/O focus list is an array indicating which jobs are requesting the
                        	; I/O focus. The I/O focus is user controlled by pressing CNTRL-T on the
                        	; keyboard.
                        	;
                        	; Parameters:
                        	;		a1 = task id requesting focus for
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_RequestIOFocus:
FFFC2574 00106D13		ldi		$t0,#1
FFFC2578 013D1D33		sll		$t0,$t0,$a1
FFFC257C 00009637		ldt		$t1,IOFocusTbl			; Is the task already included in the IO focus?
FFFC2580 C1060613
FFFC2584 00060633
FFFC2588 00062D83
FFFC258C 01ADFE33		and		$t2,$t1,$t0					; test bit
FFFC2590 060E1263		bne		$t2,$x0,.ret				; If so, don't add again
FFFC2594 01ADEDB3		or		$t1,$t1,$t0					; set bit indicator
FFFC2598 00009637		stt		$t1,IOFocusTbl
FFFC259C C1060613
FFFC25A0 00060633
FFFC25A4 01B62023
FFFC25A8 00009637		ldt		$t0,IOFocusNdx			; get current index
FFFC25AC C0860613
FFFC25B0 00060633
FFFC25B4 00062D03
FFFC25B8 00A99D93		sll		$t1,$a1,#LOG_TCBSZ	; t1 = pointer to TCB
FFFC25BC 020D5063		bge		$t0,$x0,.notEmpty		; is there one? (!= -1)
FFFC25C0 00009637		stt		$t1,IOFocusNdx			; no current index, so set equal to requester
FFFC25C4 C0860613
FFFC25C8 00060633
FFFC25CC 01B62023
FFFC25D0 2FBDA423		stt		$t1,IOF_NEXT[$t1]		; and loop back to self
FFFC25D4 2FBDA823		stt		$t1,IOF_PREV[$t1]
FFFC25D8 00000E63		bra		.ret
                        	.notEmpty:
FFFC25DC 2F0D2E03		ldt		$t2,IOF_PREV[$t0]		; insert t1 into focus ring
FFFC25E0 2FCDA823		stt		$t2,IOF_PREV[$t1]
FFFC25E4 2FADA423		stt		$t0,IOF_NEXT[$t1]
FFFC25E8 2F0D2E03		ldt		$t2,IOF_PREV[$t0]
FFFC25EC 2FBE2423		stt		$t1,IOF_NEXT[$t2]
FFFC25F0 2FBD2823		stt		$t1,IOF_PREV[$t0]
                        	.ret:
FFFC25F4 00006813		ldi		$v0,#E_Ok
FFFC25F8 E28FF06F		jmp		ERETx
                        	
                        	;-----------------------------------------------------------------------------
                        	; ReleaseIOFocus called when the task no longer desires to be on the I/O
                        	; focus list.
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_ReleaseIOFocus:
                        		
FFFC25FC 18101873		csrrw	v0,#$181,x0
FFFC2600 00F87813		and		v0,v0,#15
FFFC2604 000869B3	mov		a1,v0
                        		; fall into ForceReleaseIOFocus
                        	
                        	;-----------------------------------------------------------------------------
                        	; Releasing the I/O focus causes the focus to switch if the running job
                        	; had the I/O focus.
                        	; ForceReleaseIOFocus forces the release of the IO focus for a job
                        	; different than the one currently running.
                        	; 
                        	; Stack Space:
                        	;		2 words
                        	; Parameters:
                        	;		a1 = task id to release
                        	; Returns:
                        	;		v0 = E_Ok
                        	;-----------------------------------------------------------------------------
                        	
                        	FMTK_ForceReleaseIOFocus:
FFFC2608 00106D13		ldi		$t0,#1
FFFC260C 013D1D33		sll		$t0,$t0,$a1
FFFC2610 00009637		ldt		$t1,IOFocusTbl
FFFC2614 C1060613
FFFC2618 00060633
FFFC261C 00062D83
FFFC2620 01ADFE33		and		$t2,$t1,$t0				; test bit for task
FFFC2624 080E0663		beq		$t2,$x0,.noFocus	; does it even have the focus?
FFFC2628 FFFD4D13		xor		$t0,$t0,#-1				; get inverted mask
FFFC262C 01ADFDB3		and		$t1,$t1,$t0				; clear bit for task
FFFC2630 00009637		stt		$t1,IOFocusTbl
FFFC2634 C1060613
FFFC2638 00060633
FFFC263C 01B62023
FFFC2640 00009637		ldt		$t1,IOFocusNdx		; check if the focus being released is the current
FFFC2644 C0860613
FFFC2648 00060633
FFFC264C 00062D83
FFFC2650 00A99D13		sll		$t0,$a1,#LOG_TCBSZ	; io focus. If so, switch focus
FFFC2654 03BD1063		bne		$t0,$t1,.notSame
FFFC2658 FF870713		sub		$sp,$sp,#8
FFFC265C 01A72023		stt		$t0,[$sp]
FFFC2660 01B72223		stt		$t1,4[$sp]
FFFC2664 ED1FF0EF		call	SwitchIOFocusHelper
FFFC2668 00072D03		ldt		$t0,[$sp]
FFFC266C 00472D83		ldt		$t1,4[$sp]
FFFC2670 00870713		add		$sp,$sp,#8
                        	.notSame:
FFFC2674 2E8D2E03		ldt		$t2,IOF_NEXT[$t0]
FFFC2678 020E4C63		blt		$t2,$x0,.done
FFFC267C 01AE0A63		beq		$t2,$t0,.pjSame
FFFC2680 2F0D2D83		ldt		$t1,IOF_PREV[$t0]
FFFC2684 2FBE2823		stt		$t1,IOF_PREV[$t2]
FFFC2688 2FCDA423		stt		$t2,IOF_NEXT[$t1]
FFFC268C 00000C63		bra		.0001
                        	.pjSame:
FFFC2690 FFF06D93		ldi		$t1,#-1
FFFC2694 00009637		stt		$t1,IOFocusNdx
FFFC2698 C0860613
FFFC269C 00060633
FFFC26A0 01B62023
                        	.0001:
FFFC26A4 FFF06D93		ldi		$t1,#-1
FFFC26A8 2FBD2423		stt		$t1,IOF_NEXT[$t0]	
FFFC26AC 2FBD2823		stt		$t1,IOF_PREV[$t0]	
                        	.done:
                        	.noFocus:
FFFC26B0 00006813		ldi		$v0,#E_Ok
FFFC26B4 D6CFF06F		jmp		ERETx
